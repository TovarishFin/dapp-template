{
  "name": "dapp-template",
  "version": "0.0.1",
  "description": "A template for a DApp running on ethereum",
  "scripts": {
    "lint:js": "yarn eslint ./src ./test",
    "start": "yarn run clean:js && NODE_ENV=development babel-watch server/index.js",
    "start:prod": "yarn run build && yarn run serve",
    "test:contracts": "yarn run clean:contracts && truffle test",
    "serve": "node buildServer/index.js",
    "build": "yarn run build:client && yarn run build:server && yarn run build:node",
    "build:client": "rimraf buildClient && webpack --progress --config webpack/client.prod.js",
    "build:server": "rimraf buildServer && webpack --progress --config webpack/server.prod.js",
    "build:node": "babel server -d buildServer --ignore configureStore,render",
    "clean:install": "rimraf node_modules && yarn install",
    "clean:js": "rimraf buildClient buildServer",
    "lint": "eslint --fix src server webpack",
    "format": "prettier --single-quote --semi=false --write '{src,server,webpack}/**/*.js' && yarn run lint"
  },
  "author": "TovarishFin",
  "license": "MIT",
  "devDependencies": {
    "autodll-webpack-plugin": "^0.3.4",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": ">= 8",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-universal-import": "^1.2.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-watch": "^2.0.6",
    "bluebird": "^3.5.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-prettier": ">= 2.7.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-import": ">= 2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": ">= 2.3.1",
    "eslint-plugin-promise": ">= 3.6.0",
    "eslint-plugin-react-ssr": "^1.0.3",
    "eslint-plugin-redux-saga": "^0.6.0",
    "eslint-plugin-security": ">= 1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-css-chunks-webpack-plugin": "^2.0.15",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^21.2.1",
    "json-loader": "^0.5.4",
    "lint-staged": "^4.2.3",
    "prettier": "^1.4.4",
    "react-hot-loader": "^3.0.0",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-hot-server-middleware": "^0.1.0",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bignumber.js": "^5.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "eslint-plugin-jest": "^21.7.0",
