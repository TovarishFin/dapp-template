    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-jss": "^8.3.5",
    "react-particles-js": "^2.1.0",
    "react-redux": "^5.0.5",
    "react-universal-component": "^2.5.0",
    "recompose": "^0.26.0",
    "redux": "^3.7.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-first-router": "^1.9.15",
